using System;

class Program
{
    static void Main()
    {
        int[] numbers = new int[5];

        numbers[0] = 10;
        numbers[1] = 20;
        numbers[2] = 30;
        numbers[3] = 40;
        numbers[4] = 50;

        Console.WriteLine("Elements in the array:");
        foreach (int num in numbers)
        {
            Console.WriteLine(num);
        }
    }
}
